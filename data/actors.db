 Remorhaz","displayName":20,"actorLink":false,"width":2,"height":2,"lockRotation":false,"rotation":0,"alpha":1,"light":{"alpha":0.5,"angle":360,"bright":0,"coloration":1,"dim":0,"luminosity":0.5,"saturation":0,"contrast":0,"shadows":0,"animation":{"speed":5,"intensity":5,"reverse":false,"type":null},"darkness":{"min":0,"max":1},"attenuation":0.5,"color":null},"disposition":-1,"displayBars":40,"bar1":{"attribute":"attributes.hp"},"bar2":{"attribute":null},"flags":{},"randomImg":false,"texture":{"src":"assets/srd5e/img/MM/Young Remorhaz.png","scaleX":1,"scaleY":1,"offsetX":0,"offsetY":0,"rotation":0,"tint":null},"sight":{"angle":360,"enabled":true,"range":60,"brightness":1,"visionMode":"basic","color":null,"attenuation":0.1,"saturation":0,"contrast":0},"detectionModes":[]},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677178013583,"modifiedTime":1697141152588,"lastModifiedBy":"PP5XOkXk6hNdYKV4"},"folder":null,"sort":0,"_id":"edOGou2vMQ1X8f6j"}
