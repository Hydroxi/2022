[],"folder":null,"sort":0,"flags":{"srd5e":{"page":"items.html","source":"PHB","hash":"backpack_phb"}},"system":{"description":{"value":"\n\t\t<div class=\"rd__b  rd__b--2\"><p>A <i>backpack</i> can hold one cubic foot or 30 pounds of gear. You can also strap items, such as a bedroll or a coil of rope, to the outside of a <i>backpack</i>.</p></div>","chat":"","unidentified":""},"source":"PHB","quantity":1,"weight":5,"price":2,"attunement":0,"equipped":true,"rarity":"","identified":false,"capacity":{"type":"weight","value":30,"weightless":false},"currency":{"cp":0,"sp":0,"ep":0,"gp":0,"pp":0},"damage":{"parts":[]},"proficient":true,"consume":{"type":"","target":"","amount":null},"range":{"value":5,"long":0,"units":"ft"},"critical":{"threshold":null,"damage":null}},"ownership":{"default":0,"GtfthjDLT8452NQj":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"t4fnlQAoDnDNZ40s","name":"Bedroll","type":"loot","img":"icons/sundries/flags/banner-worn-blue.webp","effects":[],"folder":null,"sort":100000,"flags":{"srd5e":{"page":"items.html","source":"PHB","hash":"bedroll_phb"}},"system":{"description":{"value":"\n\t\t","chat":"","unidentified":""},"source":"PHB","quantity":1,"weight":7,"price":1,"attunement":0,"equipped":true,"rarity":"","identified":false,"damage":{"parts":[]},"proficient":true,"consume":{"type":"","target":"","amount":null},"range":{"value":5,"long":0,"units":"ft"},"critical":{"threshold":null,"damage":null}},"ownership":{"default":0,"GtfthjDLT8452NQj":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"X1sOMnkv706u0q7T","name":"Mess Kit","type":"loot","img":"icons/tools/cooking/fork-steel-grey.webp","effects":[],"folder":null,"sort":200000,"flags":{"srd5e":{"page":"items.html","source":"PHB","hash":"mess%20kit_phb"}},"system":{"description":{"value":"\n\t\t<div class=\"rd__b  rd__b