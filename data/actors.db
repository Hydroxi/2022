on":"2.1.5","coreVersion":"10.291","createdTime":1682189638961,"modifiedTime":1682190045433,"lastModifiedBy":"UE5rpI0nj3Ve45gc"}},{"name":"Cantrip Formulas","type":"feat","system":{"description":{"value":"<div class=\"rd__b  rd__b--1\"><p><i>3rd-level wizard @variantrule[optional class features|tce|optional feature]</i></p><p>You have scribed a set of arcane formulas in your spellbook that you can use to formulate a cantrip in your mind. Whenever you finish a long rest and consult those formulas in your spellbook, you can replace one wizard cantrip you know with another cantrip from the wizard spell list.</p></div>","chat":"","unidentified":""},"source":"TCE","activation":{"type":"special","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":1,"max":"1","per":"lr","recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"other","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"flat"},"type":{"value":"","subtype":""},"requirements":"Wizard 3","recharge":{"value":null,"charged":false}},"ownership":{"default":0,"UE5rpI0nj3Ve45gc":3},"img":"modules/plutonium/media/icon/mighty-force.svg","flags":{"plutonium":{"page":"classFeature","source":"TCE","hash":"cantrip%20formulas_wizard_phb_3_tce"},"dnd5e":{"advancementOrigin":"H3xIxjJRGOsGnmWQ.zx8bmQXVNGDmnEoA"}},"effects":[],"_id":"rUINYnhDCbJnrUjP","folder":null,"sort":0,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1682189639119,"modifiedTime":1682190045184,"lastModifiedBy":"UE5rpI0nj3Ve45gc"}},{"name":"Potent Cantrip","type":"feat","system":{"description":{"value":"<div class=\"rd__b  rd__b--3\"><p>Starting at 6th level, your damaging cantrips affect even creatures that avoid the brunt of the effect. When a creature succeeds on a saving throw against your cantrip, the creature takes half the cantrip's damage (if any) but suffers no additional effect from the cantrip.</p><div class=\"rd__spc-inline-post\"></div></div>","chat":"","unidentified":""},"source":"PHB","activation":{"type":"","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":"","actionType":"","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"class","subtype":""},"requirements":"School of Evocation 6","recharge":{"value":null,"charged":false}},"ownership":{"default":0,"UE5rpI0nj3Ve45gc":3},"effects":[],"flags":{"plutonium":{"page":"subclassFeature","source":"PHB","hash":"potent%20cantrip_wizard_phb_evocation_phb_6_phb"},"dnd5e":{"advancementOrigin":"QP6Qh00pPhZiV7YX.ZGTt1arskEJwZy9W"}},"img":"icons/magic/earth/projectiles-fire-stone-salvo.webp","_id":"grhdJouAeFes9lYw","folder":null,"sort":0,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1682190044785,"modifiedTime":1682190045433,"lastModifiedBy":"UE5rpI0nj3Ve45gc"}},{"name":"Component Pouch","type":"consumable","system":{"description":{"value":"<div class=\"rd__b  rd__b--2\"><p>A <i>component pouch</i> is a small, watertight leather belt pouch that has compartments to hold all the material components and other special items you need to cast your spells, except for those components that have a specific cost (as indicated in a spell's description).</p></div>","chat":"","unidentified":""},"source":"PHB","quantity":1,"weight":2,"price":{"value":25,"denomination":"gp"},"attunement":0,"equipped":false,"rarity":"","identified":false,"activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":0,"long":0,"units":"ft"},"uses":{"value":null,"max":"","per":null,"recovery":"","autoDestroy":false},"consume":{"type":"","target":"","amount":null},"ability":null,"actionType":"util","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"consumableType":"ammo"},"img":"icons/containers/bags/pack-simple-leather-tan.webp","ownership":{"default":0,"UE5rpI0nj3Ve45gc":3},"flags":{"plutonium":{"page":"items.html","source":"PHB","hash":"component%20pouch_phb"}},"effects":[],"_id":"HzRxwJ2chNo5ATKF","folder":null,"sort":200000,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1682190045796,"modifiedTime":1685652563315,"lastModifiedBy":"PP5XOkXk6hNdYKV4"}},{"name":"Backpack","type":"backpack","system":{"description":{"value":"<div class=\"rd__b  rd__b--2\"><p>A <i>backpack</i> can hold one cubic foot or 30 pounds of gear. You can also strap items, such as a bedroll or a coil of rope, to the outside of a <i>backpack</i>.</p></div>","chat":"","unidentified":""},"source":"PHB","quantity":1,"weight":5,"price":{"value":2,"denomination":"gp"},"attunement":0,"equipped":true,"rarity":"","identified":false,"capacity":{"type":"weight","value":30,"weightless":false},"currency":{"cp":0,"sp":0,"ep":0,"gp":0,"pp":0}},"img":"icons/containers/bags/pack-simple-leather.webp","ownership":{"default":0,"UE5rpI0nj3Ve45gc":3},"flags":{"plutonium":{"page":"items.html","source":"PHB","hash":"backpack_phb"}},"effects":[],"_id":"RB1sfFB6y0teQF5w","folder":null,"sort":0,"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1682190046372,"modifiedTime":1682193398298,"lastModifiedBy":"PP5XOkXk6hNdYKV4"}},{"name":"Bedroll","type"